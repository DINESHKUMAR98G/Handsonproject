<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="box" class="org.hcl3.Box" autowire="byType">
		<property name="length" value="10"></property>
		<property name="width" value="12"></property>
	</bean>

	<bean name="user" class="org.hcl3.User">
		<property name="name" value="Suresh"></property>
		<property name="password" value="suresh"></property>
	</bean>

</beans>
